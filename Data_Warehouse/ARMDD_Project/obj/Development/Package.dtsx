  Products 
  LEFT JOIN ProductSubcategories ON Products.ProductSubcategoryID = ProductSubcategories.ProductSubcategoryID 
  LEFT JOIN ProductCategories ON ProductSubcategories.ProductCategoryID = ProductCategories.ProductCategoryID 
  LEFT JOIN UnitMeasures AS SizeUnitMeasures ON Products.SizeUnitMeasureCode = SizeUnitMeasures.UnitMeasureCode 
  LEFT JOIN UnitMeasures AS WeightUnitMeasures ON Products.WeightUnitMeasureCode = WeightUnitMeasures.UnitMeasureCode</property>
